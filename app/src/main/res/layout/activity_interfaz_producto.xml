<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="@color/White"    tools:context=".InterfazProducto">    <!--<androidx.appcompat.widget.Toolbar        android:id="@+id/toolbarproducto"        android:layout_width="match_parent"        android:layout_height="60dp"        app:layout_constraintBottom_toTopOf="@+id/linearLayout"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent">    </androidx.appcompat.widget.Toolbar>-->    <include        android:id="@+id/TituloCategoria"        layout="@layout/toolbarcategoria"        />    <LinearLayout        android:id="@+id/linearLayout"        android:layout_width="match_parent"        android:layout_height="match_parent"        android:layout_marginTop="60dp"        android:layout_marginBottom="80dp"        android:background="@color/White"        android:orientation="vertical"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.0"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        app:layout_constraintVertical_bias="1.0">        <TextView            android:id="@+id/temp_price"            android:layout_width="1dp"            android:layout_height="1dp"/>        <ImageView            android:id="@+id/imageProduct"            android:layout_width="180dp"            android:layout_height="180dp"            android:layout_gravity="center"            android:layout_marginTop="10dp" />        <TextView            android:id="@+id/id_product"            android:layout_width="1dp"            android:layout_height="1dp" />        <TextView            android:id="@+id/Nombre"            android:layout_width="300dp"            android:layout_height="wrap_content"            android:layout_gravity="center"            android:layout_marginTop="15dp"            android:gravity="center"            android:textStyle="bold"            android:textColor="@color/Vinotino"            android:text="nombre"            android:textSize="25dp" />        <LinearLayout            android:orientation="horizontal"            android:weightSum="3"            android:gravity="center"            android:layout_width="match_parent"            android:layout_height="40dp">            <TextView                android:text="$"                android:textColor="#000"                android:gravity="center"                android:textSize="20dp"                android:layout_width="wrap_content"                android:layout_height="match_parent" />            <TextView                android:id="@+id/precio"                android:layout_width="wrap_content"                android:layout_height="match_parent"                android:textColor="#000"                android:layout_gravity="start"                android:gravity="center"                android:text="@string/preciopor"                android:textSize="20dp" />            <TextView                android:text="/"                android:textSize="20dp"                android:textColor="#000"                android:gravity="center"                android:layout_width="wrap_content"                android:layout_height="match_parent" />            <TextView                android:id="@+id/producto_unidad"                android:gravity="center"                android:textColor="#000"                android:textSize="20dp"                android:layout_width="wrap_content"                android:layout_height="match_parent" />        </LinearLayout>        <LinearLayout            android:layout_width="match_parent"            android:layout_height="70dp"            android:orientation="horizontal"            android:background="@color/White"            android:layout_marginTop="30dp"            android:weightSum="2">            <LinearLayout                android:layout_weight="1"                android:layout_marginLeft="5dp"                android:layout_width="match_parent"                android:layout_height="match_parent">                <EditText                    android:id="@+id/num_cantidad"                    android:layout_width="120dp"                    android:textColor="@color/Vinotino"                    android:backgroundTint="@color/Vinotino"                    android:maxLength="6"                    android:layout_height="match_parent"                    android:hint="Cantidad"/>                <TextView                    android:id="@+id/product_type"                    android:layout_marginLeft="10dp"                    android:textColor="#000"                    android:layout_width="wrap_content"                    android:layout_height="match_parent"                    android:gravity="center"                    android:text="ml"                    android:textSize="25dp"                    />            </LinearLayout>            <Button                android:id="@+id/agregarBolsa"                android:layout_weight="1"                android:text="@string/agregar_a_bolsa"                android:textSize="16dp"                android:textColor="@color/White"                android:textStyle="bold"                android:background="@drawable/boton_redondo"                android:layout_width="match_parent"                android:layout_height="50dp"                android:layout_gravity="center"                android:layout_marginRight="10dp"                />        </LinearLayout>    </LinearLayout>    <LinearLayout        android:layout_width="342dp"        android:layout_height="47dp"        android:layout_marginStart="8dp"        android:layout_marginTop="8dp"        android:layout_marginEnd="8dp"        android:layout_marginBottom="8dp"        android:background="@drawable/contorno"        android:orientation="horizontal"        android:weightSum="2"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/linearLayout">        <TextView            android:layout_width="wrap_content"            android:layout_height="match_parent"            android:layout_weight="1"            android:gravity="center"            android:text="Precio Total"            android:textColor="#000"            android:textSize="20dp"            android:textStyle="bold" />        <TextView            android:layout_width="wrap_content"            android:layout_height="match_parent"            android:gravity="center"            android:text="$"            android:textColor="#000"            android:textSize="20dp" />        <TextView            android:id="@+id/preciototalproducto"            android:layout_width="wrap_content"            android:layout_height="40dp"            android:gravity="center"            android:hint="321"            android:textColor="#000"            android:textSize="20dp" />    </LinearLayout>    <!--  <RelativeLayout          android:id="@+id/footer_btns"          android:layout_width="match_parent"          android:layout_height="70dp"          android:layout_alignParentLeft="true"          android:layout_alignParentBottom="true"          app:layout_constraintBottom_toBottomOf="parent"          app:layout_constraintEnd_toEndOf="parent"          app:layout_constraintStart_toStartOf="parent"          app:layout_constraintTop_toBottomOf="@+id/linearLayout">          <LinearLayout              android:id="@+id/operations"              android:layout_width="match_parent"              android:layout_height="45dp"              android:layout_centerVertical="true"              android:layout_marginLeft="10dp"              android:layout_marginRight="10dp"              android:layout_toLeftOf="@+id/agregar"              android:orientation="horizontal"              android:weightSum="3">              <RelativeLayout                  android:layout_width="0dp"                  android:layout_height="match_parent"                  android:layout_alignStart="@+id/operations"                  android:layout_alignTop="@+id/operations"                  android:layout_weight="1">                  <TextView                      android:id="@+id/restar_cantidad"                      android:layout_width="30dp"                      android:layout_height="wrap_content"                      android:layout_centerInParent="true"                      android:gravity="center"                      android:text="-"                      android:textSize="35dp" />              </RelativeLayout>              <RelativeLayout                  android:layout_width="0dp"                  android:layout_height="match_parent"                  android:layout_weight="1">                  <View                      android:layout_width="1dp"                      android:layout_height="25dp"                      android:layout_centerVertical="true"                      android:layout_marginTop="8dp"                      android:layout_marginBottom="8dp"                      android:background="#e1e1e1" />                  <EditText                      android:id="@+id/number_item"                      android:layout_width="wrap_content"                      android:layout_height="wrap_content"                      android:gravity="center"                      android:inputType="numberDecimal"                      android:text="cant"                      android:textSize="20dp" />                  <TextView                      android:layout_width="20dp"                      android:layout_height="match_parent"                      android:layout_marginLeft="-18dp"                      android:layout_marginEnd="12dp"                      android:layout_marginRight="-65dp"                      android:layout_toStartOf="@+id/viewr"                      android:layout_toLeftOf="@+id/number_item"                      android:gravity="center"                      android:text="lb"                      android:textSize="20dp" />                  <View                      android:id="@+id/viewr"                      android:layout_width="1dp"                      android:layout_height="25dp"                      android:layout_alignParentRight="true"                      android:layout_centerVertical="true"                      android:layout_marginTop="8dp"                      android:layout_marginBottom="8dp"                      android:background="#e1e1e1" />              </RelativeLayout>              <RelativeLayout                  android:layout_width="0dp"                  android:layout_height="match_parent"                  android:layout_alignParentRight="true"                  android:layout_weight="1">                  <TextView                      android:id="@+id/add_cantidad"                      android:layout_width="wrap_content"                      android:layout_height="wrap_content"                      android:layout_centerInParent="true"                      android:text="+"                      android:textSize="35dp" />              </RelativeLayout>          </LinearLayout>          <RelativeLayout              android:id="@+id/agregar"              android:layout_width="150dp"              android:layout_height="45dp"              android:layout_alignParentRight="true"              android:layout_centerVertical="true"              android:layout_marginRight="10dp">              <TextView                  android:id="@+id/agregar_text"                  android:layout_width="wrap_content"                  android:layout_height="wrap_content"                  android:layout_centerHorizontal="true"                  android:layout_centerVertical="true"                  android:text="Agregar"                  android:textColor="@color/colorPrimaryDark"                  android:textSize="17sp" />          </RelativeLayout>      </RelativeLayout>--></androidx.constraintlayout.widget.ConstraintLayout>